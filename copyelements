#include<stdio.h>
#include<string.h>
int main()
{
	int i, arr[100],brr[100],n;
	printf("enter the no.of elements : \n");
	scanf("%d",&n);
	printf("array A elements are : ");
	for( i=0;i<n;i++)
	scanf("%d",&arr[i]);
	for(i=0;i<n;i++)
	{
		brr[i]=arr[i];
	}
	printf("array B elements are : ");
	for( i=0;i<n;i++)
	printf(" %d",arr[i]);
	
	
}
	 
	  

